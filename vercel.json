{
  "version": 2,
  "builds": [
    {
      "src": "geomap_project/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": { "cache-control": "s-maxage=31536000" },
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "geomap_project/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "geomap_project.settings"
  }
}
